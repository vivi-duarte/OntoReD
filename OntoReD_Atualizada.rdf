        <untitled-ontology-3:descricao>&lt;ul&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;&lt;span style=&quot;text-align: left; color: rgb(0, 0, 0);&quot;&gt;&lt;strong&gt;O sistema de software foi aprimorado para implementar a fatia e a fatia est&amp;aacute; pronta para teste.&lt;/strong&gt;&lt;/span&gt;&lt;/li&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;&lt;strong&gt;O c&amp;oacute;digo cumpre a fatia:&lt;/strong&gt; O c&amp;oacute;digo dos elementos de implementa&amp;ccedil;&amp;atilde;o cumpre as responsabilidades exigidas pela fatia.&lt;/li&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;&lt;strong&gt;Os elementos de implementa&amp;ccedil;&amp;atilde;o s&amp;atilde;o verificados:&amp;nbsp;&lt;/strong&gt;Os elementos de implementa&amp;ccedil;&amp;atilde;o foram verificados individualmente e demonstraram cumprir seu papel dentro da fatia.&amp;nbsp;&lt;/li&gt;
&lt;/ul&gt;</untitled-ontology-3:descricao>
        <untitled-ontology-3:nome>&lt;p&gt;&lt;strong&gt;&lt;span style=&quot;font-size: 14px;&quot;&gt;Implementado&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;</untitled-ontology-3:nome>
        <untitled-ontology-3:sequencia>&lt;p&gt;4&lt;/p&gt;</untitled-ontology-3:sequencia>
    </owl:NamedIndividual>
    


    <!-- http://www.semanticweb.org/vivid/ontologies/2023/2/untitled-ontology-3#ImplementarSistema -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/vivid/ontologies/2023/2/untitled-ontology-3#ImplementarSistema">
        <rdf:type rdf:resource="http://www.semanticweb.org/vivid/ontologies/2023/2/untitled-ontology-3#EspacoAtividade"/>
        <untitled-ontology-3:Icone>&lt;p&gt;Icone Espaco Atividade&lt;/p&gt;</untitled-ontology-3:Icone>
        <untitled-ontology-3:descricao>&lt;div style=&quot;text-align: start;color: rgb(0, 0, 0);font-size: 16px;&quot;&gt;
    &lt;p style=&quot;font-size: 1em;&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;Construa um sistema implementando, testando e integrando um ou mais elementos do sistema.&amp;nbsp;&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;
    &lt;ul&gt;
        &lt;li style=&quot;font-size: 12px;&quot;&gt;Isso inclui corre&amp;ccedil;&amp;atilde;o de bugs e testes unit&amp;aacute;rios.&lt;/li&gt;
    &lt;/ul&gt;
&lt;/div&gt;
&lt;p style=&quot;text-align: start;color: rgb(0, 0, 0);font-size: 16px;&quot;&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&lt;strong&gt;Implemente o sistema para:&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;
&lt;ul style=&quot;text-align: start;color: rgb(0, 0, 0);font-size: 16px;&quot;&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;Criar um sistema funcional.&lt;/li&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;Desenvolver, integrar e testar os elementos do sistema.&lt;/li&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;Aumentar o n&amp;uacute;mero de requisitos implementados.&lt;/li&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;Corrigir defeitos.&lt;/li&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;Melhorar o sistema.&lt;/li&gt;
&lt;/ul&gt;</untitled-ontology-3:descricao>
        <untitled-ontology-3:nome>&lt;p&gt;&lt;strong&gt;&lt;span style=&quot;font-size: 14px;&quot;&gt;Implementar o sistema&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;</untitled-ontology-3:nome>
        <untitled-ontology-3:referenciaRE>&lt;div style=&quot;text-align: start;color: rgb(0, 0, 0);font-size: 16px;&quot;&gt;
    &lt;ul style=&quot;text-align: start;color: rgb(0, 0, 0);font-size: 16px;&quot;&gt;
        &lt;li style=&quot;font-size: 12px;&quot;&gt;Essence Standard (OMG), consultar&amp;nbsp;&lt;u&gt;&lt;a href=&quot;http://www.omg.org/spec/Essence/1.1/&quot; target=&quot;_blank&quot; style=&quot;color: rgb(75, 110, 198);&quot;&gt;http://www.omg.org/spec/Essence/1.1/&lt;/a&gt;&lt;/u&gt;&lt;/li&gt;
        &lt;li style=&quot;font-size: 12px;&quot;&gt;A Ess&amp;ecirc;ncia da Engenharia de Software: Aplicando o Kernel SEMAT, por Ivar Jacobson, Pan-Wei Ng, Paul E. McMahon, Ian Spence e Svante Lidman, Addison-Wesley 2013.&lt;/li&gt;
    &lt;/ul&gt;
&lt;/div&gt;</untitled-ontology-3:referenciaRE>
    </owl:NamedIndividual>
    


