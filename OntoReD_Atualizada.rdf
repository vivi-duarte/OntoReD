    <!-- http://www.semanticweb.org/vivid/ontologies/2023/2/untitled-ontology-3#AnalisarFH -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/vivid/ontologies/2023/2/untitled-ontology-3#AnalisarFH">
