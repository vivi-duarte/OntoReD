    <!-- http://www.semanticweb.org/vivid/ontologies/2023/2/untitled-ontology-3#EntenderRequisitos -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/vivid/ontologies/2023/2/untitled-ontology-3#EntenderRequisitos">
        <rdf:type rdf:resource="http://www.semanticweb.org/vivid/ontologies/2023/2/untitled-ontology-3#EspacoAtividade"/>
        <untitled-ontology-3:Icone>&lt;p&gt;Icone Espaco Atividade&lt;/p&gt;</untitled-ontology-3:Icone>
        <untitled-ontology-3:descricao>&lt;div style=&quot;text-align: start;color: rgb(0, 0, 0);font-size: 16px;&quot;&gt;
    &lt;p style=&quot;font-size: 1em;&quot;&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&lt;strong&gt;Estabele&amp;ccedil;a um entendimento compartilhado sobre o *** o sistema a ser produzido deve fazer.&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;
&lt;/div&gt;
&lt;p style=&quot;text-align: start;color: rgb(0, 0, 0);font-size: 16px;&quot;&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&lt;strong&gt;Entender os requisitos para:&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;
&lt;ul style=&quot;text-align: start;color: rgb(0, 0, 0);font-size: 16px;&quot;&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;Escopo do sistema.&lt;/li&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;Entender como o sistema ir&amp;aacute; gerar valor.&lt;/li&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;Combinar o que o sistema far&amp;aacute;.&lt;/li&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;Identificar maneiras espec&amp;iacute;ficas de usar e testar o sistema.&lt;/li&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;Conduzir o desenvolvimento do sistema.&lt;/li&gt;
&lt;/ul&gt;
&lt;h1 style=&quot;text-align: start;color: rgb(0, 0, 0);font-size: 2.375em;&quot;&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;Atividades dispon&amp;iacute;veis&lt;/span&gt;&lt;/h1&gt;
&lt;ul style=&quot;text-align: start;color: rgb(0, 0, 0);font-size: 16px;&quot;&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;&lt;u&gt;&lt;a href=&quot;https://practicelibrary.ivarjacobson.com/sites/default/files/embedded-practices/Use_Case_2.0_Essentials_HTML_guidelines6/html/_bwhEYSyJEeGkkOC4K2RI6A/Find%20Actors%20and%20Use%20Cases_gd.html&quot; title=&quot;Concorde com os objetivos e o valor do sistema, identificando formas de usá-lo e testá-lo.&quot; style=&quot;color: rgb(75, 110, 198);&quot;&gt;Encontrar atores e casos de uso&lt;/a&gt;&lt;/u&gt;:&lt;p style=&quot;font-size: 1em;&quot;&gt;Concorde com os objetivos e o valor do sistema, identificando formas de us&amp;aacute;-lo e test&amp;aacute;-lo.&lt;/p&gt;
    &lt;/li&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;&lt;u&gt;&lt;a href=&quot;https://practicelibrary.ivarjacobson.com/sites/default/files/embedded-practices/Use_Case_2.0_Essentials_HTML_guidelines6/html/_l6jqcUFZEeG1ZNmea40YGw/Inspect%20and%20Adapt%20the%20Use%20Cases_gd.html&quot; title=&quot;Lide com as mudanças e acompanhe o progresso ajustando o modelo de casos de uso, os casos de uso e as fatias de casos de uso.&quot; style=&quot;color: rgb(75, 110, 198);&quot;&gt;Inspecionar e adapte os casos de uso&lt;/a&gt;&lt;/u&gt;:&lt;p style=&quot;font-size: 1em;&quot;&gt;Lide com ** mudan&amp;ccedil;as e ********* o progresso ajustando o modelo de casos de uso, os casos de uso e ** fatias de ***** de uso.&lt;/p&gt;
    &lt;/li&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;&lt;u&gt;&lt;a href=&quot;https://practicelibrary.ivarjacobson.com/sites/default/files/embedded-practices/Use_Case_2.0_Essentials_HTML_guidelines6/html/_m71tUSyJEeGkkOC4K2RI6A/Prepare%20a%20Use-Case%20Slice_gd.html&quot; title=&quot;Uma fatia de caso de uso é preparada para desenvolvimento elaborando sua narrativa e casos de teste.&quot; style=&quot;color: rgb(75, 110, 198);&quot;&gt;******** *** fatia de caso de uso&lt;/a&gt;&lt;/u&gt;:&lt;p style=&quot;font-size: 1em;&quot;&gt;*** fatia de caso de uso &amp;eacute; preparada para desenvolvimento elaborando sua narrativa e casos de teste.&lt;/p&gt;
    &lt;/li&gt;
