    <!-- http://www.semanticweb.org/vivid/ontologies/2023/2/untitled-ontology-3#TestarSistema -->

    <owl:NamedIndividual rdf:about="http://www.semanticweb.org/vivid/ontologies/2023/2/untitled-ontology-3#TestarSistema">
        <rdf:type rdf:resource="http://www.semanticweb.org/vivid/ontologies/2023/2/untitled-ontology-3#EspacoAtividade"/>
        <untitled-ontology-3:Icone>&lt;p&gt;Icone Espaco Atividade&lt;/p&gt;</untitled-ontology-3:Icone>
        <untitled-ontology-3:descricao>&lt;div style=&quot;text-align: start;color: rgb(0, 0, 0);font-size: 16px;&quot;&gt;
    &lt;p style=&quot;font-size: 1em;&quot;&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&lt;strong&gt;Verifique se o sistema produzido atende aos requisitos das partes interessadas.&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;
&lt;/div&gt;
&lt;p style=&quot;text-align: start;color: rgb(0, 0, 0);font-size: 16px;&quot;&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;&lt;strong&gt;Teste o sistema para:&lt;/strong&gt;&lt;/span&gt;&lt;/p&gt;
&lt;ul style=&quot;text-align: start;color: rgb(0, 0, 0);font-size: 16px;&quot;&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;Verificar se o sistema de software atende aos requisitos.&lt;/li&gt;
    &lt;li style=&quot;font-size: 12px;&quot;&gt;Identificar quaisquer defeitos no sistema de software.&lt;/li&gt;
&lt;/ul&gt;

&lt;h1 style=&quot;text-align: start;color: rgb(0, 0, 0);font-size: 2.375em;&quot;&gt;&lt;span style=&quot;font-size: 12px;&quot;&gt;Objetivos&lt;/span&gt;&lt;/h1&gt;
